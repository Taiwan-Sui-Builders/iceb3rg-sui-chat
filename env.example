# Environment Variables Example
# Copy this file to .env.local and fill in your values

# Sui Network Configuration
# Options: devnet, testnet, mainnet
# Default: devnet
NEXT_PUBLIC_SUI_NETWORK=devnet

# Smart Contract Package ID
# This is the package ID of your deployed smart contract
# Example: 0x1234567890abcdef1234567890abcdef1234567890abcdef1234567890abcdef
NEXT_PUBLIC_PACKAGE_ID=

# AppConfig Shared Object ID
# This is the shared object ID for the AppConfig
# Example: 0xabcdef1234567890abcdef1234567890abcdef1234567890abcdef1234567890
NEXT_PUBLIC_APP_CONFIG_ID=

# ProfileRegistry Shared Object ID
# This is the shared object ID for the ProfileRegistry
# Example: 0x9876543210fedcba9876543210fedcba9876543210fedcba9876543210fedcba
NEXT_PUBLIC_PROFILE_REGISTRY_ID=

